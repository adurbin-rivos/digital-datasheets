{
    "$id": "https://github.com/chromeos/digital-datasheets/blob/main/part-spec/semiconductor.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "semiconductor",
    "oneOf": [
        {
            "title": "mosfet",
            "type": "object",
            "required": [
                "partType",
                "manufacturer",
                "mpn",
                "datasheetVersion",
                "type",
                "channel"
            ],
            "properties": {
                "componentID": {
                    "description": "common component identifying information, such as mpn",
                    "$ref": "definitions.json#/componentID"
                },
                "mosfetType": {
                    "description": "type of mosfet",
                    "examples": [
                        "smallSignal",
                        "power"
                    ],
                    "type": "string"
                },
                "channel": {
                    "description": "doping of a transistor's channel - describes whether a transistor is n-type or p-type",
                    "enum": [
                        "nType",
                        "pType"
                    ],
                    "type": "string"
                },
                "transistorCount": {
                    "description": "number of transistors in the package",
                    "type": "number"
                },
                "vgsMax": {
                    "description": "maximum gate to source voltage difference that can be continously applied to a mosfet",
                    "comment": "units of volts",
                    "$ref": "definitions.json#/unit"
                },
                "vdsMax": {
                    "description": "maximum drain to source voltage difference that can be continously applied to a mosfet",
                    "comment": "units of volts",
                    "$ref": "definitions.json#/unit"
                },
                "vgsThMax": {
                    "description": "maximum gate to source voltage difference required to produce a conducting path between drain and source",
                    "comment": "units of volts",
                    "$ref": "definitions.json#/unit"
                },
                "iDrain": {
                    "description": "maximum continous DC current that can flow through a mosfet channel",
                    "comment": "units of current",
                    "$ref": "definitions.json#/unit"
                },
                "idPulsed": {
                    "description": "maximum pulsed DC current that can flow through a mosfet channel",
                    "comment": "units of current",
                    "$ref": "definitions.json#/unit"
                },
                "rdson": {
                    "description": "drain source on resistance of a mosfet",
                    "$comment": "conditions include vds, vgs, temperature",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "qg": {
                    "description": "total gate charge of a mosfet",
                    "$comment": "conditions include drain-source voltage and drain current",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "qgd": {
                    "description": "gate to drain charge of a mosfet",
                    "comment": "units of nC",
                    "$ref": "definitions.json#/unit"
                },
                "qgs": {
                    "description": "gate to source charge of a mosfet",
                    "comment": "units of nC",
                    "$ref": "definitions.json#/unit"
                },
                "idVsVds": {
                    "description": "graph of drain current (Id) vs drain source voltage (Vds)",
                    "$ref": "graph.json#/graphDefiniton"
                },
                "idVsVgs": {
                    "description": "graph of drain current (Id) vs gate source voltage (Vgs)",
                    "$ref": "graph.json#/graphDefiniton"
                },
                "pins": {
                    "description": "array of pin objects with associated properties",
                    "type": "array",
                    "items": {
                        "$ref": "definitions.json#/pinSpec"
                    }
                }
            }
        },
        {
            "title": "diode",
            "type": "object",
            "required": [
                "componentID"
            ],
            "properties": {
                "componentID": {
                    "description": "common component identifying information, such as mpn",
                    "$ref": "definitions.json#/componentID"
                },
                "type": {
                    "description": "type of diode",
                    "examples": [
                        "zener",
                        "schottky",
                        "esd"
                    ],
                    "type": "string"
                },
                "vf": {
                    "description": "forward voltage of a diode",
                    "comment": "conditions include: forward current of diode under which a given forward voltage was measured",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "ifm": {
                    "description": "maximum continuous forward current a diode can support",
                    "comment": "units of amps",
                    "$ref": "definitions.json#/unit"
                },
                "ifrm": {
                    "description": "maximum repetitive peak forward current a diode can support",
                    "comment": "units of amps",
                    "$ref": "definitions.json#/unit"
                },
                "ifsm": {
                    "description": "maximum non-repetitive surge forward current a diode can support",
                    "comment": "units of amps",
                    "$ref": "definitions.json#/unit"
                },
                "vbr": {
                    "description": "breakdown voltage of a diode",
                    "comment": "units of volts",
                    "$ref": "definitions.json#/unit"
                },
                "ir": {
                    "description": "reverse leakage current",
                    "comment": "conditions include: reverse bias voltage under which reverse leakage current occurs",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "vz": {
                    "description": "breakdown voltage of a zener diode",
                    "comment": "conditions include: minimum reverse current condition under which breakdown voltage of a zener diode is achieved",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "vrm": {
                    "description": "reverse standoff voltage of a tvs diode",
                    "comment": "units of volts",
                    "$ref": "definitions.json#/unit"
                },
                "vcl": {
                    "description": "clamping voltage of a tvs diode",
                    "comment": "conditions include: surge current condition under which a tvs clamping voltage occurs",
                    "$ref": "definitions.json#/conditionalProperty"
                },
                "esdProtection": {
                    "description": "whether a diode has built in esd protection",
                    "type": "boolean"
                },
                "package": {
                    "description": "package size of resistor",
                    "$ref": "definitions.json#/package"
                },
                "pins": {
                    "description": "array of pin objects with associated properties",
                    "type": "array",
                    "items": {
                        "$ref": "definitions.json#/pinSpec"
                    }
                }
            }
        }
    ]
}